[1mdiff --git a/05_Data Analysis.sql b/05_Data Analysis.sql[m
[1mnew file mode 100644[m
[1mindex 0000000..49be66a[m
[1m--- /dev/null[m
[1m+++ b/05_Data Analysis.sql[m	
[36m@@ -0,0 +1,167 @@[m
[32m+[m[32m# üìä Data Analysis[m[41m [m
[32m+[m
[32m+[m[32m## 1Ô∏è‚É£ Counting Total Users[m
[32m+[m[32m```[m
[32m+[m[32mSELECT COUNT(DISTINCT(Id)) AS users_count[m
[32m+[m[32mFROM `spheric-engine-432411-p0.case_study_2.daily_activity_cleaned`;[m
[32m+[m[32m```[m
[32m+[m[32m‚úÖ **Total Users:** 35[m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## 2Ô∏è‚É£ Average Daily Steps & Calories Per Weekday[m
[32m+[m[32m```[m
[32m+[m[32mWITH week_day_table AS ([m
[32m+[m[32m    SELECT[m
[32m+[m[32m      Id,[m
[32m+[m[32m      TotalSteps,[m
[32m+[m[32m      Calories,[m
[32m+[m[32m      FORMAT_TIMESTAMP('%A', ActivityDate) AS week_day,[m
[32m+[m[32m      EXTRACT(DAYOFWEEK FROM ActivityDate) AS week_day_number[m
[32m+[m[32m    FROM `spheric-engine-432411-p0.case_study_2.daily_activity_cleaned`[m
[32m+[m[32m)[m
[32m+[m[32mSELECT[m
[32m+[m[32m  Id,[m
[32m+[m[32m  week_day,[m
[32m+[m[32m  week_day_number,[m
[32m+[m[32m  ROUND(AVG(TotalSteps), 2) AS avg_daily_steps,[m
[32m+[m[32m  ROUND(AVG(Calories), 2) AS avg_daily_calories[m
[32m+[m[32mFROM week_day_table[m
[32m+[m[32mGROUP BY Id, week_day, week_day_number[m
[32m+[m[32mORDER BY week_day_number ASC;[m
[32m+[m[32m```[m
[32m+[m[32m‚úÖ **Average Daily Steps:** Below recommended range (8000-10000)[m[41m  [m
[32m+[m[32m‚úÖ **Average Calories Burned:** Within adult range (1200-2400)[m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## 3Ô∏è‚É£ Categorizing Users by Activity Level[m
[32m+[m[32m**Classifications:**[m
[32m+[m[32m- **Sedentary Active:** `< 5000` steps[m
[32m+[m[32m- **Lightly Active:** `5000 - 7499` steps[m
[32m+[m[32m- **Fairly Active:** `7500 - 9999` steps[m
[32m+[m[32m- **Very Active:** `>= 10000` steps[m
[32m+[m
[32m+[m[32m### üìù Method 1: Using CROSS JOIN[m
[32m+[m[32m```[m
[32m+[m[32mWITH average_steps_table AS ([m
[32m+[m[32m    SELECT Id, ROUND(AVG(TotalSteps), 2) AS average_steps[m
[32m+[m[32m    FROM `spheric-engine-432411-p0.case_study_2.daily_activity_cleaned`[m
[32m+[m[32m    WHERE NOT (SedentaryMinutes = 1440 AND TotalDistance = 0 AND TotalSteps = 0)[m
[32m+[m[32m    GROUP BY Id[m
[32m+[m[32m)[m
[32m+[m[32mSELECT *[m[41m [m
[32m+[m[32mFROM ([m
[32m+[m[32m  SELECT ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM average_steps_table), 2) AS sedentary_active_percentage FROM average_steps_table WHERE average_steps < 5000[m
[32m+[m[32m) sedentary_active_users[m
[32m+[m[32mCROSS JOIN ([m
[32m+[m[32m  SELECT ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM average_steps_table), 2) AS lightly_active_percentage FROM average_steps_table WHERE average_steps BETWEEN 5000 AND 7499[m
[32m+[m[32m) lightly_active_users[m
[32m+[m[32mCROSS JOIN ([m
[32m+[m[32m  SELECT ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM average_steps_table), 2) AS fairly_active_percentage FROM average_steps_table WHERE average_steps BETWEEN 7500 AND 9999[m
[32m+[m[32m) fairly_active_users[m
[32m+[m[32mCROSS JOIN ([m
[32m+[m[32m  SELECT ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM average_steps_table), 2) AS very_active_percentage FROM average_steps_table WHERE average_steps >= 10000[m
[32m+[m[32m) very_active_users;[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m### üìù Method 2: Using CASE Statement[m
[32m+[m[32m```[m
[32m+[m[32mWITH average_steps_table AS ([m
[32m+[m[32m    SELECT Id, ROUND(AVG(TotalSteps), 2) AS average_steps[m
[32m+[m[32m    FROM `spheric-engine-432411-p0.case_study_2.daily_activity_cleaned`[m
[32m+[m[32m    WHERE NOT (SedentaryMinutes = 1440 AND TotalDistance = 0 AND TotalSteps = 0)[m
[32m+[m[32m    GROUP BY Id[m
[32m+[m[32m)[m
[32m+[m[32mSELECT[m
[32m+[m[32m  ROUND(SUM(CASE WHEN average_steps < 5000 THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) AS sedentary_active_percentage,[m
[32m+[m[32m  ROUND(SUM(CASE WHEN average_steps BETWEEN 5000 AND 7499 THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) AS lightly_active_percentage,[m
[32m+[m[32m  ROUND(SUM(CASE WHEN average_steps BETWEEN 7500 AND 9999 THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) AS fairly_active_percentage,[m
[32m+[m[32m  ROUND(SUM(CASE WHEN average_steps >= 10000 THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) AS very_active_percentage[m
[32m+[m[32mFROM average_steps_table;[m
[32m+[m[32m```[m
[32m+[m[32m‚úÖ **Sedentary Users:** 37.14%[m[41m  [m
[32m+[m[32m‚úÖ **Lightly Active Users:** 20.0%[m[41m  [m
[32m+[m[32m‚úÖ **Fairly Active Users:** 22.86%[m[41m  [m
[32m+[m[32m‚úÖ **Very Active Users:** 20.0%[m[41m  [m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## 4Ô∏è‚É£ Hourly Steps per Weekday[m
[32m+[m[32m```[m
[32m+[m[32mSELECT[m
[32m+[m[32m  Id,[m
[32m+[m[32m  FORMAT_TIMESTAMP('%A', ActivityHour) AS week_day,[m
[32m+[m[32m  EXTRACT(DAYOFWEEK FROM ActivityHour) AS week_day_num,[m[41m  [m
[32m+[m[32m  ROUND(AVG(StepTotal),2) AS avg_steps,[m
[32m+[m[32m  EXTRACT(HOUR FROM ActivityHour) AS day_hour[m
[32m+[m[32mFROM `spheric-engine-432411-p0.case_study_2.hourly_steps`[m
[32m+[m[32mGROUP BY Id, week_day, week_day_num, day_hour[m
[32m+[m[32mORDER BY week_day_num, day_hour ASC;[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## 5Ô∏è‚É£ Correlation Between Steps, Sleep, and Calories[m
[32m+[m[32m```[m
[32m+[m[32mWITH steps_table AS ([m
[32m+[m[32m  SELECT[m
[32m+[m[32m    Id,[m
[32m+[m[32m    FORMAT_TIMESTAMP('%A', ActivityHour) AS week_day,[m
[32m+[m[32m    EXTRACT(DAYOFWEEK FROM ActivityHour) AS week_day_num,[m
[32m+[m[32m    ROUND(AVG(StepTotal), 2) AS avg_steps_per_hour,[m
[32m+[m[32m    SUM(StepTotal) AS total_daily_steps,[m
[32m+[m[32m    EXTRACT(DATE FROM ActivityHour) AS activity_date[m
[32m+[m[32m  FROM `spheric-engine-432411-p0.case_study_2.hourly_steps`[m
[32m+[m[32m  GROUP BY Id, week_day, week_day_num, activity_date[m
[32m+[m[32m),[m
[32m+[m[32msleep_table AS ([m
[32m+[m[32m  SELECT[m
[32m+[m[32m    Id,[m
[32m+[m[32m    FORMAT_TIMESTAMP('%A', date) AS week_day,[m
[32m+[m[32m    EXTRACT(DAYOFWEEK FROM date) AS week_day_num,[m
[32m+[m[32m    SUM(value) AS total_sleep_minutes,[m
[32m+[m[32m    EXTRACT(DATE FROM date) AS sleep_date[m
[32m+[m[32m  FROM `spheric-engine-432411-p0.case_study_2.minute_sleep`[m
[32m+[m[32m  GROUP BY Id, week_day, week_day_num, sleep_date[m
[32m+[m[32m)[m
[32m+[m[32mSELECT[m
[32m+[m[32m  steps_table.Id,[m
[32m+[m[32m  steps_table.week_day,[m
[32m+[m[32m  steps_table.week_day_num,[m
[32m+[m[32m  steps_table.total_daily_steps,[m
[32m+[m[32m  sleep_table.total_sleep_minutes[m
[32m+[m[32mFROM steps_table[m
[32m+[m[32mLEFT JOIN sleep_table[m
[32m+[m[32mON steps_table.Id = sleep_table.Id AND steps_table.activity_date = sleep_table.sleep_date[m
[32m+[m[32mORDER BY steps_table.week_day_num;[m
[32m+[m[32m```[m
[32m+[m
[32m+[m[32m---[m
[32m+[m
[32m+[m[32m## 5Ô∏è‚É£ Device Usage Distribution (Low, Moderate, High Use)[m
[32m+[m[32m```sql[m
[32m+[m[32mWITH user_usage_counts AS ([m
[32m+[m[32m  SELECT Id, COUNT(*) AS usage_count[m
[32m+[m[32m  FROM `spheric-engine-432411-p0.case_study_2.daily_activity_cleaned`[m
[32m+[m[32m  GROUP BY Id[m
[32m+[m[32m),[m
[32m+[m[32musage_levels AS ([m
[32m+[m[32m  SELECT Id,[m
[32m+[m[32m    CASE[m[41m [m
[32m+[m[32m      WHEN usage_count >= 45 THEN 'High Use'[m
[32m+[m[32m      WHEN usage_count BETWEEN 16 AND 44 THEN 'Moderate Use'[m
[32m+[m[32m      ELSE 'Low Use'[m
[32m+[m[32m    END AS usage_level[m
[32m+[m[32m  FROM user_usage_counts[m
[32m+[m[32m)[m
[32m+[m[32mSELECT usage_level, COUNT(*) AS count, ROUND(COUNT(*) * 100.0 / (SELECT COUNT(*) FROM usage_levels), 2) AS percentage[m
[32m+[m[32mFROM usage_levels[m
[32m+[m[32mGROUP BY usage_level[m
[32m+[m[32mORDER BY percentage DESC;[m
[32m+[m[32m```[m
[32m+[m[32m‚úÖ **Low Use:** 5.71%[m[41m  [m
[32m+[m[32m‚úÖ **Moderate Use:** 77.15%[m[41m  [m
[32m+[m[32m‚úÖ **High Use:** 17.14%[m
[32m+[m
[32m+[m[32m---[m
